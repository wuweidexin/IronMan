<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.chen.entity.Manager" table="tb_manager">
  <id name="id" type="integer">
  <column name="id"/>
   <generator class="native"/>
  </id>
  <property name="managerID" type="string" column="managerID"/>
  <property name="managerName" type="string" column="managerName"/>
  <property name="managerPassword" type="string" column="managerPassword"/>
  
 </class>
</hibernate-mapping>
